    "seedrandom": "^3.0.5",
    "uuid": "^9.0.1"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md,svelte,yml}": "prettier --write"
  }
